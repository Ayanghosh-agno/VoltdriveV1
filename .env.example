# Salesforce Configuration
# Copy this file to .env and fill in your actual Salesforce credentials

# Salesforce Connected App Credentials
VITE_SALESFORCE_CLIENT_ID=your_consumer_key_here
VITE_SALESFORCE_CLIENT_SECRET=your_consumer_secret_here

# Salesforce User Credentials (for automatic authentication)
VITE_SALESFORCE_USERNAME=your_salesforce_username@domain.com
VITE_SALESFORCE_PASSWORD=your_salesforce_password
VITE_SALESFORCE_SECURITY_TOKEN=your_security_token

# Salesforce Instance URL
VITE_SALESFORCE_INSTANCE_URL=https://your-instance.salesforce.com

# Note: In production, use environment variables instead of .env files for security